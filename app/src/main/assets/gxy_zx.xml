        <radio
            name="zxsfh"
            initValue="9"
            label="左心室肥厚" >

            <item
                name="是"
                code="1"/>

            <item
                name="否"
                code="0"/>

            <item
                name="不详"
                code="9"/>
        </radio>

        <radio
            name="dmbzh"
