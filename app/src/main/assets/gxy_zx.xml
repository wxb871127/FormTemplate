            <input
                name="gmdzdb2"
                domain="gmdzdb2 >= 0 &amp;&amp; gmdzdb2 &lt;= 10000"
                inputType="numberDecimal"
                label="高密度脂蛋白"
                precision="2"
                unit="mg/dl" />

            <input
                name="dmdzdb1"
                domain="dmdzdb1 >= 0 &amp;&amp; dmdzdb1 &lt;= 10000"
                inputType="numberDecimal"
                label="低密度脂蛋白"
                precision="2"
                unit="mmol/l" />

            <input
                name="dmdzdb2"
                domain="dmdzdb2 >= 0 &amp;&amp; dmdzdb2 &lt;= 10000"
                inputType="numberDecimal"
